{
  "theme": "GitHub",
  "selectedAuthType": "oauth-personal",
  "preferredEditor": "neovim",
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "github": {
      "httpUrl": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "${GITHUB_TOKEN}"
      },
      "timeout": 5000
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    }
  },
  "checkpointing": {
    "enabled": false
  }
}
